{% extends 'dashboardSponsor.html.twig' %}

{% block title %}Annonce{% endblock %}

{% block body %}
    <h1>Annonce</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ annonce.id }}</td>
            </tr>
            <tr>
                <th>Contenu</th>
                <td>{{ annonce.contenu }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ annonce.date ? annonce.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                <td>{{ annonce.photo }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_annonce_index_sponsor', {'id': user.id}) }}">back to list</a>


    {{ include('annonce/_delete_form.html.twig') }}
{% endblock %}
