                            <i class="nav-icon fas fa-copy"></i>
                            <p>
                            <p>Group</p>
                            <i class="fas fa-angle-left right"></i>
                            </p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a href={{ path('app_role_index') }} class="nav-link">
                            <i class="nav-icon fas fa-copy"></i>
                            <p>
                            <p>Role</p>
                            <i class="fas fa-angle-left right"></i>
                            </p>
                        </a>
                    </li>
                    <li class="nav-header">Content Managment </li>

                    <li class="nav-item">
