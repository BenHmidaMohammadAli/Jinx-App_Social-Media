{% extends 'dashboardAdmin.html.twig' %}

{% block title %}Role{% endblock %}

{% block body %}
    <h1>Role</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ role.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ role.name }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ role.status ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>RoleName</th>
                <td>{{ role.roleName }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_role_index') }}">back to list</a>

{% endblock %}
